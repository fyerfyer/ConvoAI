{
  "name": "frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/frontend",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project frontend --web",
  "targets": {
    "build": {
      "command": "next build --webpack",
      "options": {
        "cwd": "apps/frontend"
      },
      "outputs": [
        "{projectRoot}/.next/!(cache)/**/*",
        "{projectRoot}/.next/!(cache)"
      ]
    },
    "dev": {
      "continuous": true,
      "command": "next dev --webpack --port 4200",
      "options": {
        "cwd": "apps/frontend"
      }
    },
    "docker-serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker compose up discord-frontend"
      }
    }
  }
}
